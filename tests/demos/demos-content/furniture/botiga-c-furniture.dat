.custom-btn-style-1 .wp-block-button__link {
		font-size: 0.7rem !important;
		padding: 13px 35px;
		border-width: 1px;
		font-weight: 600;
		text-transform: none;
}

.custom-btn-style-2 .wp-block-button__link {
		border-width: 1px;
